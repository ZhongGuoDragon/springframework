        <value>3</value>
    </util:list>
    <bean id="blankdisc" class="com.tom.soundsystem.BlankDisc"
    p:title="bbbbbbbbbb" p:artist="aaaaaaaa" p:list-ref="tracklist"
    >
        <constructor-arg value="11111"/>
        <constructor-arg value="22222222222"/>
    </bean>
